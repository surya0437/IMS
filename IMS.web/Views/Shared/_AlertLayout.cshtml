@if(TempData["success"] != null){

<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
<script>
        swal({
            title: "Good job!",
            text: @TempData["success"],
            icon: "success",
        });
</script>
}


@if (TempData["error"] != null)
{

    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
    <script>
        swal({
            title: "Sorry!",
            icon: "warning",
            buttons: true,
            dangerMode: true,
        });
    </script>
}